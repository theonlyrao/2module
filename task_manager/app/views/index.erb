<div class="col-md-12">

  <div class="row">
    <h1>All Tasks</h1>
  </div>
      <table class="table">
        <tr>
          <th>Task ID</th>
          <th>Task Title</th>
        </tr>
        <tr>
         <% @tasks.each do |task| %>
            <td><%= task.id %>:</td>
            <td> <a href="/tasks/<%= task.id %>"><%= task.title %></a></td>
        </tr>
       <% end %>
     </table>
 </div>
